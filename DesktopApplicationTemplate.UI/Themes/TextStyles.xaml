<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <!-- Text styles derived from MainWindow-Design.layout.json -->
    <Style x:Key="TextStyle0" TargetType="TextBlock">
        <Setter Property="FontFamily" Value="Forte" />
        <Setter Property="FontSize" Value="279" />
    </Style>
    <Style x:Key="TextStyle1" TargetType="TextBlock">
        <Setter Property="FontFamily" Value="Helvetica" />
        <Setter Property="FontSize" Value="279" />
    </Style>
    <Style x:Key="TextStyle2" TargetType="TextBlock">
        <Setter Property="FontFamily" Value="Helvetica" />
        <Setter Property="FontSize" Value="281" />
    </Style>
    <Style x:Key="TextStyle3" TargetType="TextBlock">
        <Setter Property="FontFamily" Value="Helvetica" />
        <Setter Property="FontSize" Value="68" />
    </Style>
    <Style x:Key="TextStyle4" TargetType="TextBlock">
        <Setter Property="FontFamily" Value="Helvetica" />
        <Setter Property="FontSize" Value="68" />
        <Setter Property="FontWeight" Value="Bold" />
    </Style>
    <Style x:Key="TextStyle5" TargetType="TextBlock">
        <!-- TODO: Missing font family; using default -->
        <Setter Property="FontSize" Value="10" />
    </Style>
</ResourceDictionary>
