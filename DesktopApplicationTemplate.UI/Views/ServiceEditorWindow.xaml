<Window x:Class="DesktopApplicationTemplate.UI.Views.ServiceEditorWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Service Editor" Width="900" Height="700"
        WindowStartupLocation="CenterOwner"
        AllowsTransparency="True"
        Style="{DynamicResource BubblyWindowStyle}">
    <Window.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="../Themes/BubblyWindow.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Window.Resources>
    <ScrollViewer VerticalScrollBarVisibility="Auto" HorizontalScrollBarVisibility="Auto">
        <Frame x:Name="EditorFrame" NavigationUIVisibility="Hidden" />
    </ScrollViewer>
</Window>
